doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1= title
        p
            a(href="/update-cobj") Add to this table

        if data && data.length > 0
            table
                thead
                    tr
                        each property in Object.keys(data[0].properties)
                            th= property
                tbody
                    each home in data
                        tr
                            each property in Object.keys(home.properties)
                                td= home.properties[property]
        else
            p No homes found 